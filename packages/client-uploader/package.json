{
  "name": "@flamedeck/upload",
  "version": "0.2.2",
  "description": "Client library for uploading traces to Flamedeck.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/flamedeck-org/flamedeck.git",
    "directory": "packages/client-uploader"
  },
  "homepage": "https://github.com/flamedeck-org/flamedeck/tree/main/packages/client-uploader#readme",
  "bugs": {
    "url": "https://github.com/flamedeck-org/flamedeck/issues"
  },
  "keywords": [
    "flamedeck",
    "trace",
    "profile",
    "upload"
  ],
  "author": "Flamedeck Team",
  "license": "ISC",
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
